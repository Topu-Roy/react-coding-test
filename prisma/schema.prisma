generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  acceptedTerms Boolean
  sector        Sector?
}

model Sector {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  value   Int    @unique
  label   String
  user    User @relation(fields: [userId], references: [id])
  userId  String @unique @db.ObjectId
}