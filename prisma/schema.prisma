generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Sector {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  value   Int @unique
  label   String
  user    User[]
}

model User {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  acceptedTerms Boolean
  sector        Sector   @relation(fields: [sectorId], references: [id])
  sectorId      String @db.ObjectId
}